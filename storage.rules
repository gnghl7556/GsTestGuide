rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /previews/{fileName} {
      allow read: if true;
      allow write: if false;
    }

    match /agreements/{testNumber}/{fileName} {
      allow read, write: if request.auth != null;
    }

    match /checklist-previews/{label}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null;
    }

    match /sample-downloads/{label}/{fileName} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}
